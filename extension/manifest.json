{
    "manifest_version": 3,
    "name": "Goodreads Review Summary",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["*://www.goodreads.com/book/show/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  